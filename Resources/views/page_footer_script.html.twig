{% block footer_js %}
    <!-- JavaScript at the bottom for fast page loading -->

    <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ asset( "bundles/acmeezblog/js/jquery-1.7.1.min.js" ) }}"><\/script>')</script>

    <!-- scripts concatenated and minified via build script -->
    <script src="{{ asset( "bundles/acmeezblog/js/custom.js" ) }}"></script>

    <!-- superfish -->
    <script src="{{ asset( "bundles/acmeezblog/js/superfish-1.4.8/js/hoverIntent.js" ) }}"></script>
    <script src="{{ asset( "bundles/acmeezblog/js/superfish-1.4.8/js/superfish.js" ) }}"></script>
    <script src="{{ asset( "bundles/acmeezblog/js/superfish-1.4.8/js/supersubs.js" ) }}"></script>
    <!-- ENDS superfish -->

    <script src="{{ asset( "bundles/acmeezblog/js/tweet/jquery.tweet.js" ) }}" ></script>
    <script src="{{ asset( "bundles/acmeezblog/js/jquery.isotope.min.js" ) }}"></script>
    <script src="{{ asset( "bundles/acmeezblog/js/jquery.nivo.slider.js" ) }}" ></script>
    <script src="{{ asset( "bundles/acmeezblog/js/css3-mediaqueries.js" ) }}"></script>
    <script src="{{ asset( "bundles/acmeezblog/js/tabs.js" ) }}"></script>
    <script src="{{ asset( "bundles/acmeezblog/js/poshytip-1.1/src/jquery.poshytip.min.js" ) }}"></script>
    <!-- end scripts -->
